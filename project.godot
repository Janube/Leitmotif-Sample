config_version=5

[application]

config/name="Leitmotif"
run/main_scene="res://game_level.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.svg"

[autoload]

Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"Blue": "res://Art/Characters/Blue/Blue.dch",
"Grey": "res://Art/Characters/Grey/Grey.dch",
"Player": "res://addons/dialogic/test-project/Characters/Player.dch",
"Red": "res://red.dch",
"Yellow": "res://Art/Characters/Yellow/Yellow.dch"
}
directories/dtl_directory={
"Blue": "res://Art/Characters/Blue/Blue.dtl",
"Epilogue": "res://addons/dialogic/test-project/Timelines/Epilogue.dtl",
"Grey": "res://Art/Characters/Grey/Grey.dtl",
"Player": "res://addons/dialogic/test-project/Timelines/Player.dtl",
"Player Death": "res://addons/dialogic/test-project/Timelines/Player Death.dtl",
"Preload": "res://addons/dialogic/test-project/Timelines/Preload.dtl",
"Red": "res://Red.dtl",
"Yellow": "res://Art/Characters/Yellow/Yellow.dtl"
}
layout/style_list=["res://addons/dialogic/Style.tres", "res://addons/dialogic/Style Backup.tres"]
